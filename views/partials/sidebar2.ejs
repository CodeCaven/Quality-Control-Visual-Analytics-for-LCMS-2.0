<!-- Top/menu -->
<!-- Header -->
<div class="w3-top" id="header_container" style="z-index:5;">
  
  <div class="w3-panel w3-light-grey headerBorder" style="margin-top:0em; margin-left: 0em; margin-right: 0em;">
      
      
    
     
      <span>
          <button id="openNav" class="w3-button w3-large w3-display-topleft w3-white" 
          style="margin-left:1em;" onclick="w3_open()">&#9776</button>
        <h1 class="w3-large w3-dark-gray w3-center"  style="text-shadow:1px 1px 0 #444">
          
        <b class="w3-center">
          <%= title %> QUALITY CONTROL</b></h1></span>
    
  
    
    
    <div class="header">
        
      <span class="w3-large w3-text-dark-gray"><strong>Venue: </strong><span id="venueHeader"></span></span>
      <span class="w3-xlarge w3-center w3-bold w3-text-dark-gray" id="viewLabel"></span>
      <span class="w3-large w3-text-dark-gray" style="right:0;"><strong>Machine: </strong><span id="machineHeader"></span></span>
      
    </div>
  </div>
</div>